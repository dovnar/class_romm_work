#include <iostream>

using namespace std;

int pow(int number, int pow)
{
	int result = 1;
	for (int i = 0; i < pow; i++)
	{
		result *= number;
	}
	return result;
}

int main()
{
	int result;
	int number;
	cin >> number;
	for (int i = 1; i <= 10; i++)
	{
		result = pow(number, i);
		cout << number << " ^ " << i << " = " << result << endl;
	}
	return 0;
}
